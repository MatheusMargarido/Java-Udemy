package entities;

public class Quartos {
	
	
	private String nome;
	private String email;
	private String quantoAlugado;
	
	public Quartos(String nome, String email, String quantoAlugado) {
		this.nome = nome;
		this.email = email;
		this.quantoAlugado = quantoAlugado;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getQuantoAlugado() {
		return quantoAlugado;
	}

	public void setQuantoAlugado(String quantoAlugado) {
		this.quantoAlugado = quantoAlugado;
	}
	

	
	
	
	
	
	
}
